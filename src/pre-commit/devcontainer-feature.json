{
  "id": "pre-commit",
  "name": "pre-commit",
  "version": "2.1.3",
  "documentationURL": "https://github.com/nikaro/features/tree/main/src/pre-commit",
  "description": "A framework for managing and maintaining multi-language pre-commit hooks.",
  "options": {
    "version": {
      "default": "",
