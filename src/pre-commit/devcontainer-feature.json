    },
    "debug": {
      "default": false,
      "description": "Enable debug logging.",
      "type": "boolean"
    }
  },
  "installsAfter": [
    "ghcr.io/nikaro/features/packages",
    "ghcr.io/nikaro/features/pipx",
    "ghcr.io/nikaro/features/pyenv",
    "ghcr.io/nikaro/features/python"
  ]
}
